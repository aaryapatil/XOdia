      <script type="text/javascript" src="/static/js/jquery-2.1.3.min.js"></script>
      <script type="text/javascript" src="/static/js/materialize.min.js"></script>
      <script type ="text/javascript" src="/static/js/sweet-alert.min.js"></script>
      <script type ="text/javascript" src="/static/js/alertify.js"></script>
<script>

  $(document).ready(function() {
    $('select').material_select();
  });

  var l = 0;
  if(l){
    alertify.log("SELECT DIFFERENT PLAYERS");
  }


 {
      
        var flag=0;
        var xchange = 30;
        var ychange = 30;     
        var Canvas = function(canvasEl,width,height){
            this.el = canvasEl;
            this.el.width = width;
            this.el.height = height;
            this.ctx = canvasEl.getContext("2d");           
          }
        var canvas = new Canvas(document.querySelector("#mycanvas"),580,580);

        function drawpoints(){
        var i = 50;
        var j = 50;
        for(i=50;i<530;i+=xchange)
        {             
            for(j=50;j<530;j+=ychange)
            { 
              canvas.ctx.lineWidth = 3;
              canvas.ctx.strokeStyle = "#222222";
              canvas.ctx.strokeRect(i,j,1,1);               
            }
        }     
  }
      }              
      var check = document.getElementById("log").textContent;
      myData = check.split(" ");
    var length = myData.length;
    
    var loop = 0;
    var gamespeed = document.getElementById("slider").value;
    function speedChange(){  
      clearInterval(window.invalid);      
      gamespeed = document.getElementById("slider").value;
      toast("game speed changed to "+gamespeed,2000);            
      
      if(document.getElementById("gamestatus").value!="Game is Paused!" && document.getElementById("gamestatus").value!="Game is About to begin!" && document.getElementById("gamestatus").value!="Game Over!\nClick 'Start/Replay' to Replay"){
        setInterval(gameplay(),gamespeed);
        document.getElementById("gamestatus").value="Game has begun!";  
      }

    } 
      

    
      
      function init(){
            {

              drawpoints();
              var str = myData[loop];
              str = str.split(".");           
              var string = (str[1]+"."+str[2]+"."+str[3]);
            //var string = str[1]+"."str[2]+"."+str[3];
            var error = parseInt(str[0]);
            var player = parseInt(str[4]);
            var player_1_score = str[5];
            var player_2_score = str[6];
            var flagbox=parseInt(str[7]);
            var boxx=parseInt(str[8]);
            var boxy=parseInt(str[9]);
            
            if(error==0)//no error in the bot hence continue the rendering
              {
                
                var orientation = string[0];
                var point = parseInt(str[2]);
                var line = parseInt(str[3]);
                
                if(orientation=='h' || orientation=='H')
                  {
                    
                    var x1 = 50+(line*xchange);             
                    var y1 = 50+(point*ychange);
                    var x2 = x1+xchange;
                    var y2 = y1;
                    clearline(x1,y1,x2,y2);
                    drawline(x1,y1,x2,y2,player);
                    drawpoints();                    
                  }
                else
                  {             
                    
                    var x1 = 50+(point*xchange);              
                    var y1 = 50+(line*ychange);
                    var x2 = x1;
                    var y2 = y1+ychange;
                    clearline(x1,y1,x2,y2);
                    drawline(x1,y1,x2,y2,player);
                    drawpoints();                   
                    //get x1,y1,x2,y2 call drawline
                    //find if box is to be printed using checkbox if yes then print
                  }
                  if(flagbox==1){
                    clearbox(boxy,boxx);
                    drawbox(boxy,boxx,player);
                    drawpoints();
                    scoreupdate(player_1_score,player_2_score);
                  }
                //get the string render the line and scores
                var toshow = "Bot "+player+" played the move "+str[1]+"."+str[2]+"."+str[3]; 
                loggenrator(toshow);                
                loop+=1;                
              }
            else
              {  
                
                var temp = myData[loop].split(".");
                console.log(temp);
                scoreupdate(temp[5],temp[6]);
                if(parseInt(player)==1)
                {
                  var winnerr = "tingu";
                  var loser = "TheGamer";
                }else{
                 var winnerr = "TheGamer";
                 var loser =  "tingu";
                }

                if(parseInt(temp[5])>parseInt(temp[6]))
                {
                  var winnerr = "tingu"; //here should be player 1 ka variable
                  var loser = "TheGamer"; ////here should be player 2 ka variable
                }else{
                 var winnerr = "TheGamer"; //here should be player 2 ka variable
                 var loser =  "tingu"; //here should be player 1 ka variable
                }               
                                if(error==1){
                  swal("Bot "+winnerr+ " is declared winner", "Bot " + loser + " did not compile successfully","warning");                 
                }
                if(error==2){
                  swal("Bot "+winnerr+" is declared winner","Bot " + loser + " did not respond in time","warning");                                      
                }
                if(error==3){
                  swal("Bot "+winnerr+" is declared winner","Bot " + loser+" tried some nasty methods to override our systems hence is disqualified","warning");                                      
                }
                if(error==4){
                  swal("Bot "+winnerr +" is declared winner","Bot "+ loser+" returned invalid","warning");                     
                }               
                if(error==5){

                   if(player_1_score == player_2_score){
                    swal("Game is over", "the game ended in a TIE" , "warning");
                   }

                else
                  {           
                    swal("Game is over", "the winner is Bot "+winnerr,"success");
                  }
                  window.flag=0;                    
                }
                loop=length;
                document.getElementById("gamestatus").value="Game Over!\nClick 'Start/Replay' to Replay";
              }
          }             
      

      }   

      function gameplay(){

        gamespeed = document.getElementById("slider").value;
        var status = 1;
        window.flag=status;
      var stop = setInterval(function (){
        window.invalid = stop;
        
        if(loop<length){
        init(); 
      }
        else
          {
          clearInterval(stop);
          window.flag = 2;          
          }
        
      }, gamespeed);


    }

    function scoreupdate(player_1_score,player_2_score){
        document.getElementById("p1").innerHTML="tingu : "+player_1_score;
        document.getElementById("p2").innerHTML="TheGamer : "+player_2_score;

      }

    function clearline(x1,y1,x2,y2){
      canvas.ctx.beginPath();
      canvas.ctx.lineWidth = 4;
      canvas.ctx.moveTo(x1,y1);
      canvas.ctx.lineTo(x2,y2);      
      canvas.ctx.strokeStyle = '#eeeeee';      
      canvas.ctx.stroke();
      canvas.ctx.closePath();
    }     
      
    function drawline(x1,y1,x2,y2,player){
      canvas.ctx.beginPath();
      canvas.ctx.lineWidth = 3;
      canvas.ctx.moveTo(x1,y1);
      canvas.ctx.lineTo(x2,y2);
      if(player==1){
        canvas.ctx.strokeStyle = '#E01E63';
      }
      else{
        canvas.ctx.strokeStyle = '#03A9F4';
      }
      canvas.ctx.stroke();
      canvas.ctx.closePath();
    }

      function drawbox(x1,y1,player){
        canvas.ctx.beginPath();
      canvas.ctx.moveTo(x1,y1);
      if(player==1){
        
        canvas.ctx.fillStyle="#ff8a80";
          canvas.ctx.fillRect(x1*30+52,y1*30+52,26,26);
      }
      else{
        
        canvas.ctx.fillStyle="#81f4f8";
          canvas.ctx.fillRect(x1*30+52,y1*30+52,26,26);        
        }    
        
     }
     function clearbox(x1,y1){
      canvas.ctx.beginPath();
      canvas.ctx.moveTo(x1,y1);       
      canvas.ctx.fillStyle="#eeeeee";
      canvas.ctx.fillRect(x1*30+52,y1*30+52,26,26);
      canvas.ctx.closePath();        
     }


      function interrupt(){
        if(document.getElementById("gamestatus").value=="Game is About to begin!"){
          toast("Click on 'Start/Replay' to start the game",2000);
        }
        else{
        gamespeed = document.getElementById("slider").value;
        var btn = document.querySelector("#myBtn");
        if(btn.value == "Pause"){
          
          document.getElementById("myBtn").value = "Resume";          
          clearInterval(window.invalid);
          alertify.log("Game is Paused",1000);
          document.getElementById("gamestatus").value="Game is Paused!"; 

        }
        else
          {
            document.getElementById("myBtn").value = "Pause";
            document.getElementById("gamestatus").value="Game is resumed!";
            alertify.log("Game is Resumed",1000);
            setInterval(gameplay(),gamespeed);      
          }
        }
      }

      function prevmove(){
        console.log(loop);
      if(loop==0){
        swal("Game didn't start yet","Previous move not available","error");
      }
      else{
        if(document.getElementById("myBtn").value == "Pause"){
          interrupt(); 
        }
        if(loop==myData.length){
          loop-=1;
        }      
          document.getElementById("myBtn").value == "Resume"
          clearInterval(window.invalid);          
          var str = myData[loop];          
          str = str.split(".");           
          var string = (str[1]+"."+str[2]+"."+str[3]);
          var error = parseInt(str[0]);
          var player = parseInt(str[4]);
          var player_1_score = str[5];
          var player_2_score = str[6];
          var flagbox=parseInt(str[7]);
          var boxx=parseInt(str[8]);
          var boxy=parseInt(str[9]);
          if(error==0)//no error in the bot hence continue the rendering
              {                
                var orientation = string[0];
                var point = parseInt(str[2]);
                var line = parseInt(str[3]);
                
                if(orientation=='h' || orientation=='H')
                  {
                    
                    var x1 = 50+(line*xchange);             
                    var y1 = 50+(point*ychange);
                    var x2 = x1+xchange;
                    var y2 = y1;
                    clearline(x1,y1,x2,y2);
                    drawpoints();
                  }
                else
                  {             
                    
                    var x1 = 50+(point*xchange);              
                    var y1 = 50+(line*ychange);
                    var x2 = x1;
                    var y2 = y1+ychange;
                    clearline(x1,y1,x2,y2);                                                           
                    drawpoints();
                  }
                  if(flagbox==1){
                    clearbox(boxy,boxx);
                    drawpoints();
                    scoreupdate(player_1_score,player_2_score);
                  }
                //get the string render the line and scores
                var toshow = "Bot "+player+" played the move "+str[1]+"."+str[2]+"."+str[3]; 
                loggenrator(toshow);                                                
              }
            loop-=1;
          }
        }      
  function nextmove(){
    if(loop==myData.length){
      swal("Game is over","Next move not available","error");
    }
    else{

      if(document.getElementById("myBtn").value == "Pause"){
          interrupt(); 
        }        
        clearInterval(window.invalid);
        init();
      }
    }
      
      function replay(){
        if(window.flag == 0){
        gameplay();
        alertify.log("Let the Games Begin!",1000);
        document.getElementById("gamestatus").value="Game has begun!";  
        }
        else if(window.flag == 2){
          location.reload();
          }
        else{
          toast("Game already Running",1000);
          toast("Click on Play/Pause to pause or play",3000);
        }
      }
    
      function loggenrator(move){     
      document.getElementById("logdisplay").value=move;
      document.getElementById("logdisplay").scrollTop = document.getElementById("log").scrollHeight;
}

      
      document.getElementById("gamestatus").value="Game is About to begin!"; 
      document.getElementById("logdisplay").value="";
      drawpoints();



